# Flutter build files
*.dill filter=lfs diff=lfs merge=lfs -text
*.dill.track.dill filter=lfs diff=lfs merge=lfs -text
*.cache.dill.track.dill filter=lfs diff=lfs merge=lfs -text

# iOS build files
*.framework/** filter=lfs diff=lfs merge=lfs -text
*.app/** filter=lfs diff=lfs merge=lfs -text
*.dylib filter=lfs diff=lfs merge=lfs -text
*.swiftmodule/** filter=lfs diff=lfs merge=lfs -text

# Android build files
*.apk filter=lfs diff=lfs merge=lfs -text
*.aab filter=lfs diff=lfs merge=lfs -text
*.dex filter=lfs diff=lfs merge=lfs -text

# Binary files
*.bin filter=lfs diff=lfs merge=lfs -text
*.so filter=lfs diff=lfs merge=lfs -text
*.dll filter=lfs diff=lfs merge=lfs -text

# Large data files
*.zip filter=lfs diff=lfs merge=lfs -text
*.tar.gz filter=lfs diff=lfs merge=lfs -text
*.7z filter=lfs diff=lfs merge=lfs -text

# Media files
*.mp4 filter=lfs diff=lfs merge=lfs -text
*.mov filter=lfs diff=lfs merge=lfs -text
*.avi filter=lfs diff=lfs merge=lfs -text
*.mp3 filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text

# Image files (if large)
*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text
*.gif filter=lfs diff=lfs merge=lfs -text
*.bmp filter=lfs diff=lfs merge=lfs -text
*.tiff filter=lfs diff=lfs merge=lfs -text

# Font files
*.ttf filter=lfs diff=lfs merge=lfs -text
*.otf filter=lfs diff=lfs merge=lfs -text
*.woff filter=lfs diff=lfs merge=lfs -text
*.woff2 filter=lfs diff=lfs merge=lfs -text

# Database files
*.db filter=lfs diff=lfs merge=lfs -text
*.sqlite filter=lfs diff=lfs merge=lfs -text
*.sqlite3 filter=lfs diff=lfs merge=lfs -text

# Log files (if large)
*.log filter=lfs diff=lfs merge=lfs -text

# Exclude build directories from LFS (they should be in .gitignore)
build/** -filter=lfs -diff=lfs -merge=lfs -text
.dart_tool/** -filter=lfs -diff=lfs -merge=lfs -text
.idea/** -filter=lfs -diff=lfs -merge=lfs -text
.vscode/** -filter=lfs -diff=lfs -merge=lfs -text 